Cell["\<\
How direct (minimal number of steps) a query can you build to find the mean \
length of counterfeit notes?\
\>", "ItemNumbered",
 CellChangeTimes->{{3.664612054528729*^9, 3.664612129129425*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"multiple", " ", "steps"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"counterfeits", "=", 
     RowBox[{
      RowBox[{"Query", "[", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"#\"Counterfeit\"", "\[Equal]", "1"}], "&"}], "]"}], "]"}], 
      "@", "swissBankNotes"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"counterfeits", "[", 
     RowBox[{"[", 
      RowBox[{"All", ",", "\"\<Length\>\""}], "]"}], "]"}], "//", 
    "Mean"}]}]}]], "Input",
 CellChangeTimes->{{3.6646114402029943`*^9, 3.664611582620558*^9}}],

